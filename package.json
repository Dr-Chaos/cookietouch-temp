{
  "name": "cookietouch",
  "version": "0.1.2",
  "description": "DofusTouch Bot with Electron React/TypeScript",
  "main": "dist/main/main.js",
  "scripts": {
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir",
    "dev": "electron-webpack dev",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' -t stylish",
    "lint:fix": "yarn lint --fix",
    "lint:check": "tslint-config-prettier-check ./tslint.json",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "sentry": "node sentry-symbols.js",
    "postinstall": "electron-builder install-app-deps",
    "release": "yarn compile && build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cookie-project/cookietouch.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack",
    "cookie",
    "cookietouch"
  ],
  "author": "Cookie Project <staff.cookietouch@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cookie-project/cookietouch/issues"
  },
  "homepage": "https://github.com/cookie-project/cookietouch#readme",
  "devDependencies": {
    "@sentry/cli": "^1.40.0",
    "@sentry/webpack-plugin": "^1.6.1",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "awesome-typescript-loader": "^5.2.0",
    "electron": "^4.0.6",
    "electron-builder": "^20.38.5",
    "electron-download": "^4.1.1",
    "electron-webpack": "^2.6.2",
    "electron-webpack-ts": "^3.1.1",
    "prettier": "^1.16.3",
    "sass-loader": "^7.1.0",
    "terser": "3.16.1",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-build-notifier": "^0.1.30"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "@sentry/electron": "^0.16.0",
    "anticaptcha": "^1.0.5",
    "axios": "^0.18.0",
    "downshift": "^3.2.6",
    "electron-log": "^3.0.1",
    "electron-updater": "^4.0.6",
    "firebase": "^5.8.5",
    "linqts": "^1.12.2",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "react": "^16.8.3",
    "react-color": "^2.17.0",
    "react-dom": "^16.8.3",
    "rxjs": "^6.4.0",
    "source-map-support": "^0.5.10",
    "typeface-roboto": "^0.0.54"
  }
}
